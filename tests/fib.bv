
fib(0) ~ 1;
fib(1) ~ 1;
fib(x) {
	x > 1
	relate fib(x - 1) + fib(x - 2)
};

myfun(0) ~ 0;
myfun(0) ~ 1;
myfun(x) {
	x > 0
	y ~ myfun(x - 1)
	relate y + 1
};

justabove(x) {
	z ~ fib(5)
	relate x + z
};


swapped(x, y) ~ (y, x);

parent('a) ~ 'b;
parent('b) ~ 'c;
parent('a) ~ 'd;

grandparent(x) {
	relate parent(parent(x))
};

ancestor(x) ~ x;
ancestor(x) {
	y ~ parent(x)
	relate ancestor(y)
};

branch('a, 'c) ~ ('x, 'y);
branch('b, 'c) ~ ('x, 'z);

calc(x) {
	y ~ 10 * fib(x) + 10
	relate y
};

compute() {
	relate fib(100)
};

%just([x]) ~ x;

%head([h, t]) ~ h;

%flip([a, b]) ~ [b, a];

%reverse([h:tail]) {
%	relate append(reverse(tail), [h])
%};
